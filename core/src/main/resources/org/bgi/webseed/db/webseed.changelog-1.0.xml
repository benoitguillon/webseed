<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

	<changeSet id="webseed-core-1.0" author="guillon" runOnChange="true">
		<createTable tableName="firm">
			<column name="id" type="BIGINT" autoIncrement="true"/>
			<column name="name" type="VARCHAR(255)"/>
			<column name="webcontext" type="VARCHAR(20)"/>
		</createTable>
	</changeSet>
	
	<changeSet id="sample-firms-1.0" author="guillon" runOnChange="true">
		<insert tableName="firm">
			<column name="id"></column>
			<column name=""></column>
		</insert>
	</changeSet>

                      
</databaseChangeLog>